def mean(sequence):
    total = 0.0
    for element in sequence:
        total = total + element
    return total/len(sequence)
short = [5, 5, 5, 5]
mean(short)
5.0
mean([5, 5, 5, 5])
5.0
mean([3, 2, 6, 1])
3.0
